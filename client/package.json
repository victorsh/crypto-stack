{
  "name": "react-template",
  "version": "1.0.0",
  "description": "A template and test environment for react.",
  "main": "index.js",
  "scripts": {
    "serve": "NODE_ENV=development webpack serve --mode development --config ./webpack-switch.config.js",
    "nodeserved": "NODE_ENV=development PORT=8080 nodemon dev-server.js",
    "nodeservep": "NODE_ENV=production PORT=3001 node dev-server.js",
    "stage": "npm run build && nodemon ./stage/server.js",
    "build": "npm run clean && webpack  --mode production --config ./webpack.config.js && cp ./dist/* ./stage/public",
    "clean": "rm -rf ./stage/public/* && rm -f ./dist/*",
    "watch": "webpack --watch",
    "electrond": "concurrently -l \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:8080 && electron ."
  },
  "author": "Victor Shahbazian",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@maticnetwork/maticjs": "^2.0.45",
    "@maticnetwork/walletconnect-provider": "^0.0.1-beta.1",
    "@randlabs/myalgo-connect": "^1.1.1",
    "@react-three/fiber": "^7.0.7",
    "axios": "^0.21.4",
    "crypto-js": "^4.1.1",
    "ethers": "^5.4.6",
    "lodash-es": "^4.17.21",
    "random-sentence": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "three": "^0.132.2",
    "tinker-bot-commons": "file:../backend/commons",
    "web3": "^1.5.2",
    "web3-eth-accounts": "^1.5.2",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "@openzeppelin/hardhat-upgrades": "^1.10.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv": "^10.0.0",
